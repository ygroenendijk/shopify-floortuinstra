{%- liquid
  # Render google_site_verifications from settings
  if settings.google_site_verification != blank
    assign google_site_verifications_arr = settings.google_site_verification | strip_newlines | split: ','
    for google_site_verification_code in google_site_verifications_arr
      if google_site_verification_code != blank
        echo '<meta name="google-site-verification" content="' | append: google_site_verification_code | strip | append: '">'
      endif
    endfor
  endif
-%}
